{{#if length}}
  <div class='d-entry-list'>
    {{#each}}
      {{partial "entries/list_item" withActions=true}}
    {{/each}}

    {{#if endOfList}}
      <div class='btn btn-default disabled d-load-more-entries'>No More Entries</div>
    {{else}}
      <div class='btn btn-default d-load-more-entries' {{action "loadMore" target="view"}}>Load More</div>
    {{/if}}    
  </div> 
{{else}}
  <div class='d-none'>No Entries</div>
{{/if}}